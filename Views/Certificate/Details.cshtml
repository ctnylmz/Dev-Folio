    <div class="overlay-mf"></div>
</div>

    <!--/ Intro Skew End /-->
    <!--/ Section Blog-Single Star /-->
    <section class="blog-wrapper sect-pt4" id="blog">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="post-box">
                        <div class="post-thumb text-center">
                            <img src="@Model.Img" class="img-fluid w-50" alt="">
                        </div>
                    <div class="post-meta text-center">
                            <h1 class="article-title mt-5">@Model.Title ~ @Model.Time</h1>
                        
                        </div>
                        <div class="article-content mt-4">
                            <p>
                             @Model.Description
                            </p>
                      
                       
                           
                         
                        </div>
                    </div>
            
            </div>
            </div>
        </div>
    </section>

